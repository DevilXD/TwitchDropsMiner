# Docker environment variables

# Directory for persistent data storage (default: ./data)
DATA_DIR=./data

# Web interface port (default: 8080)
WEB_PORT=8080

# Time zone (default: UTC)
TIMEZONE=UTC

# Docker image registry (default: ghcr.io)
DOCKER_REGISTRY=ghcr.io

# Docker image tag (default: latest)
TAG=latest

# JWT Authentication Secret (leave empty to auto-generate)
JWT_SECRET=